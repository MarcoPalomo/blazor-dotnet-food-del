<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" />
    </Found>
    <NotFound>
        <LayoutView>
            <p>Sorry, there's nothing at this address. <br> Kompè app la poko finn. Posew man pasians !</p>
        </LayoutView>
    </NotFound>
</Router>
